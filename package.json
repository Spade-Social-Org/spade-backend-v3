{
  "name": "Spade",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "_moduleAliases": {
    "~": "dist/src",
    "@src": "dist/src"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -c src/database/migrations -n ",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js ",
    "migrate": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/database/connections/default.ts",
    "create:db:entity": "sh -c 'typeorm-ts-node-esm entity:create src/$0'",
    "create:db:subscriber": "sh -c 'typeorm-ts-node-esm subscriber:create src/$0'",
    "create:db:migrations": "sh -c 'typeorm-ts-node-esm migration:create src/database/migrations/$0'",
    "generate:db:migrations": "sh -c 'ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate src/database/migrations/$0 -d src/database/connections/default.ts'",
    "run:db:migrations": "sh -c 'typeorm-ts-node-esm migration:run -d src/database/connections/default.ts'",
    "revert:db:migrations": "sh -c 'typeorm-ts-node-esm migration:revert -d src/database/connections/default.ts'",
    "show:db:migrations": "sh -c 'typeorm-ts-node-esm migration:show -d src/database/connections/default.ts'",
    "sync:db:schema": "sh -c 'typeorm-ts-node-esm schema:sync -d src/database/connections/default.ts'",
    "log:db:schema": "sh -c 'typeorm-ts-node-esm schema:log -d src/database/connections/default.ts'",
    "drop:db:schema": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js  schema:drop -d src/database/connections/default.ts",
    "query:db": "sh -c 'typeorm-ts-node-esm query $0 -d src/database/connections/default.ts'",
    "clear:db:migrations:cache": "sh -c 'typeorm-ts-node-esm cache:clear -d src/database/connections/default.ts'"
  },
  "dependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bullmq": "^10.0.1",
    "@nestjs/common": "^10.0.5",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.5",
    "@nestjs/cqrs": "^10.0.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/platform-express": "^10.0.5",
    "@nestjs/platform-socket.io": "^10.1.3",
    "@nestjs/swagger": "^7.1.1",
    "@nestjs/throttler": "^4.2.1",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.1.3",
    "@ngneat/falso": "^6.4.0",
    "@sentry/node": "^7.57.0",
    "@types/streamifier": "^0.1.0",
    "axios": "^1.4.0",
    "axios-logger": "^2.6.2",
    "bcrypt": "^5.1.0",
    "bullmq": "^4.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^1.37.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "faker": "^6.6.6",
    "handlebars": "^4.7.7",
    "joi": "^17.9.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "nestjs-pino": "^3.3.0",
    "nestjs-translator": "0.0.6",
    "nodemailer": "^6.9.3",
    "path-posix": "^1.0.0",
    "pg": "^8.11.1",
    "pino": "^8.14.1",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.1",
    "postgres": "^3.3.5",
    "reflect-metadata": "^0.1.13",
    "resend": "^0.17.1",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "streamifier": "^0.1.1",
    "typeorm": "^0.3.17",
    "typeorm-seeding": "^1.6.1",
    "until-promise": "^0.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/lodash": "^4.14.195",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.0",
    "@types/nodemailer": "^6.4.8",
    "@types/pg": "^8.10.2",
    "@types/supertest": "^2.0.12",
    "@types/until-promise": "^0.3.1",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0-alpha.1",
    "jest": "29.6.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
